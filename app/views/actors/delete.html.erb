<h1> Delete <%= @actor.first_name %> <%= @actor.last_name %></h1>
<form action="/actors/<%= @actor.id %>" method="post">
	<div>
		<label>First Name:</label>
		<input type="text" name="actor[first_name]" value="<%= @actor.first_name %>" disabled>
	</div>
	<div>
		<label>Last Name:</label>
		<input type="text" name="actor[last_name]" value="<%= @actor.last_name %>" disabled>
	</div>
	<input type="submit" value="Delete">
</form>