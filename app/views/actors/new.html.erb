<h1>Create Actor</h1>
<form action="/actors" method="post">
	<div>
		<label>First Name:</label>
		<input type="text" name="actor[first_name]">
	</div>
	<div>
		<label>Last Name:</label>
		<input type="text" name="actor[last_name]">
	</div>
	<input type="submit" value="Create">
</form>
<% if flash[:errors] %>
	<% flash[:errors].each do |e| %>
	<div><%= e %></div>
	<% end %>
<%end%>